{
  "name": "connect",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "audit:fix": "npm_config_yes=true npx yarn-audit-fix",
    "build": "next build",
    "ci:audit": "/bin/bash -c 'yarn audit --groups dependencies; [[ $? -ge 8 ]] && exit 1 || true'",
    "ci:e2e": "cypress run",
    "ci:lint": "next lint",
    "ci:unit-test": "npx jest --color --silent --detectOpenHandles --coverage --ci --reporters=default --reporters=jest-junit",
    "e2e": "NEXT_PUBLIC_PRIVACY_POLICY_URL=https://example.com/connect-privacy-policy CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "start": "node server.js",
    "test": "npx jest --detectOpenHandles",
    "prettier:fix": "yarn prettier . --check --write"
  },
  "dependencies": {
    "@next/font": "^13.4.19",
    "@radix-ui/react-accordion": "^1.1.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/nextjs": "^7.64.0",
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "next": "^13.4.19",
    "next-plausible": "^3.11.1",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-is": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "styled-components": "^6.0.8",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.4",
    "@next/eslint-plugin-next": "^13.4.19",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.26",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "axios-mock-adapter": "^1.21.5",
    "eslint": "^8.48.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "jest-junit": "^16.0.0",
    "jest-styled-components": "^7.1.1",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "optionalDependencies": {
    "cypress": "^12.17.4"
  }
}
